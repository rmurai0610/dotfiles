{
  "coc.preferences.semanticTokensHighlights": false,
  "highlight.document.enable": false,
  "python": {
    "jediEnabled": false,
    "linting": {
      "flake8Enabled": true,
      "pylintEnabled": false,
      "enabled": false
    },
    "formatting": {
      "provider": "black",
      "blackArgs": [
        "--line-length",
        "120"
      ]
    }
  },
  "clangd.trace.server": "verbose",
  "clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/14.0.3/clangd_14.0.3/bin/clangd",
  "clangd.inlayHints": {
    "enable": false
  },
  "json.format": {
    "enable": false
  }
}
